{
  "name": "panochess-manager-client",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "cross-env NODE_ENV=development vite",
    "build": "tsc && cross-env NODE_ENV=production vite build --mode production && mkdir -p /var/www/html/panochess-manager && cp -fR $(pwd)/dist/* /var/www/html/panochess-manager/",
    "build:preview": "tsc && cross-env NODE_ENV=development vite build --mode staging && sudo mkdir -p /var/www/html/staging/panochess-manager && sudo cp -fR $(pwd)/dist/* /var/www/html/staging/panochess-manager/",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint --fix src --ext ts,tsx",
    "prettier": "prettier --check \"src/**/(*.tsx|*.ts|*.css|*.scss)\"",
    "prettier:fix": "prettier --write \"src/**/(*.tsx|*.ts|*.css|*.scss)\""
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-brands-svg-icons": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@tanstack/react-query": "^5.18.1",
    "@tanstack/react-query-devtools": "^5.18.1",
    "@types/lodash.debounce": "^4.0.9",
    "@types/react-big-calendar": "^1.8.9",
    "antd": "^5.13.3",
    "avatar-initials": "^6.0.0",
    "axios": "^1.6.6",
    "clsx": "^2.1.0",
    "cross-env": "^7.0.3",
    "date-fns": "^3.3.1",
    "dayjs": "^1.11.10",
    "firebase": "^10.8.0",
    "font-awesome": "^4.7.0",
    "html-react-parser": "^5.1.1",
    "jodit-react": "^4.0.9",
    "lodash.debounce": "^4.0.8",
    "qs": "^6.11.2",
    "react": "^18.2.0",
    "react-big-calendar": "^1.11.2",
    "react-dom": "^18.2.0",
    "react-infinite-scroll-component": "^6.1.0",
    "react-number-format": "^5.3.1",
    "react-router-dom": "^6.21.3",
    "slugify": "^1.6.6",
    "swiper": "^11.0.5",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.11.6",
    "@types/qs": "^6.9.11",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "sass": "^1.69.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
}
